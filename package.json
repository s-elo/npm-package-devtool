{
  "name": "@shopee/npm-devtool",
  "version": "0.0.5",
  "description": "Npm Package Devtool",
  "author": "lichao@shopee.com",
  "bin": {
    "npt": "./bin/index.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "rollup --config --watch",
    "ts-check": "tsc --noEmit",
    "build": "rm -rf dist && rollup --config",
    "prepublishOnly": "pnpm ts-check && pnpm build",
    "release": "bumpp && npm publish",
    "version:feature": "node ./scripts/version-feature.mjs",
    "versions": "npm view @shopee/npm-devtool versions"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/debounce": "^3.0.0",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^6.5.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "rollup": "^3.28.1",
    "rollup-plugin-typescript2": "^0.35.0",
    "simple-git": "^3.19.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "concurrently": "^8.2.1",
    "debounce": "^1.2.1",
    "inquirer": "^9.2.10",
    "node-watch": "^0.7.4",
    "recursive-copy": "^2.0.14"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 16"
  },
  "packageManager": "pnpm@8.6.9"
}
